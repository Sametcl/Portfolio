@model StellerPortfolio.Models.TblContact
@{
    ViewBag.Title = "AddContact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Yeni iletişim Bilgisi Ekleme Sayfası</h3>


<form method="post">
    <label>Telefon</label>
    @Html.TextBoxFor(x=>x.Phone, new { @class = "form-control" })
    <br />
    <label>Email</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />
    <label>Adres</label>
    @Html.TextBoxFor(x => x.Address, new { @class = "form-control" })
    <br />
    <label>Harita Url</label>
    @Html.TextBoxFor(x => x.MapUrl, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-primary">Kaydet</button>
</form>
