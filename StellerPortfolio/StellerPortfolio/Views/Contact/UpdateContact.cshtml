@model StellerPortfolio.Models.TblContact

@{
    ViewBag.Title = "UpdateContact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>İletişim Güncelleme Sayfası </h3>


<form method="post">
    @Html.HiddenFor(x=>x.ContactId)
    <label>Telefon</label>
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
    <br />
    <label>Email</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />
    <label>Adres</label>
    @Html.TextBoxFor(x => x.Address, new { @class = "form-control" })
    <br />
    <label>Harita Url</label>
    @Html.TextBoxFor(x => x.MapUrl, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-primary">Güncelle</button>
</form>
